{
    "tokenColors": [
        {
            "settings": {
                "foreground": "#49483e"
            }
        },
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "foreground": "#75715E"
            }
        },
        {
            "name": "String",
            "scope": "string",
            "settings": {
                "foreground": "#998f2f"
            }
        },
        {
            "name": "Number",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#684d99"
            }
        },
        {
            "name": "Built-in constant",
            "scope": "constant.language",
            "settings": {
                "foreground": "#684d99"
            }
        },
        {
            "name": "User-defined constant",
            "scope": ["constant.character", "constant.other"],
            "settings": {
                "foreground": "#684d99"
            }
        },
        {
            "name": "Variable",
            "scope": "variable",
            "settings": {
                "fontStyle": "",
                "foreground": "#49483e"
            }
        },
        {
            "name": "Keyword",
            "scope": "keyword",
            "settings": {
                "foreground": "#f9005a"
            }
        },
        {
            "name": "Storage",
            "scope": "storage",
            "settings": {
                "fontStyle": "",
                "foreground": "#f9005a"
            }
        },
        {
            "name": "Storage type",
            "scope": "storage.type",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#0089b3"
            }
        },
        {
            "name": "Class name",
            "scope": ["entity.name.class", "entity.name.type.class", "entity.name.type"],
            "settings": {
                "fontStyle": "underline",
                "foreground": "#679c00"
            }
        },
        {
            "name": "Inherited class",
            "scope": "entity.other.inherited-class",
            "settings": {
                "fontStyle": "italic underline",
                "foreground": "#679c00"
            }
        },
        {
            "name": "Function name",
            "scope": "entity.name.function",
            "settings": {
                "fontStyle": "bold underline",
                "foreground": "#679c00"
            }
        },
        {
            "name": "Function argument",
            "scope": "variable.parameter",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#cf7000"
            }
        },
        {
            "name": "Tag name",
            "scope": "entity.name.tag",
            "settings": {
                "fontStyle": "",
                "foreground": "#f9005a"
            }
        },
        {
            "name": "Tag attribute",
            "scope": "entity.other.attribute-name",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#679c00"
            }
        },
        {
            "name": "Library function",
            "scope": "support.function",
            "settings": {
                "fontStyle": "",
                "foreground": "#0089b3"
            }
        },
        {
            "name": "Library constant",
            "scope": "support.constant",
            "settings": {
                "fontStyle": "",
                "foreground": "#0089b3"
            }
        },
        {
            "name": "Library class/type",
            "scope": ["support.type", "support.class"],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#0089b3"
            }
        },
        {
            "name": "Library variable",
            "scope": "support.other.variable",
            "settings": {
                "fontStyle": "",
                "foreground": "#49483e"
            }
        },
        {
            "name": "Invalid",
            "scope": "invalid",
            "settings": {
                "fontStyle": "",
                "foreground": "#FF0000"
            }
        },
        {
            "name": "Invalid deprecated",
            "scope": "invalid.deprecated",
            "settings": {
                "foreground": "#FF0000"
            }
        },
        {
            "name": "Type annotation",
            "scope": ["meta.type.annotation entity.name.type"],
            "settings": {
                "fontStyle": "bold underline"
            }
        },
        {
            "name": "Type annotation parameters",
            "scope": ["meta.type.annotation meta.type.parameters"],
            "settings": {
                "fontStyle": "underline",
                "foreground": "#679c00"
            }
        },
        {
            "name": "Variable parameter",
            "scope": ["variable.parameter"],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Variable other constant",
            "scope": ["variable.other.constant"],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Variable other object",
            "scope": ["variable.other.object"],
            "settings": {
                "foreground": "#2F4F4F"
            }
        },
        {
            "name": "Variable other readwrite",
            "scope": ["variable.other.readwrite"],
            "settings": {
                "foreground": "#CD853F"
            }
        },
        {
            "scope": ["meta.class punctuation.definition.block"],
            "settings": {
                "foreground": "#333333",
                "fontStyle": "bold"
            }
        },
        {
            "scope": ["meta.import meta.block punctuation.definition.block"],
            "settings": {
                "foreground": "#faaaaa"
            }
        },
        {
            "scope": ["meta.brace.round"],
            "settings": {
                "foreground": "#FF0000"
            }
        }
    ],
    "colors": {
        "editor.background": "#fafafa",
        "editor.foreground": "#49483e",
        "editorLineNumber.foreground": "#a3a3a3",
        "editorLineNumber.activeForeground": "#FA8072",
        "editor.lineHighlightBackground": "#e6e3c3",
        "editor.selectionBackground": "#ccc9ad",
        "editorWhitespace.foreground": "#49483E",
        "editorCursor.foreground": "#666663",
        "editorGhostText.foreground": "#FA8072",
        "editorSuggestWidget.foreground": "#696969",
        "editorSuggestWidget.focusHighlightForeground": "#ffffff",
        "editorSuggestWidget.selectedForeground": "#ffffff",
        "editorError.foreground": "#FA8072",
        "editorHint.border": "#FA8072",
        "editorIndentGuide.background": "#cacaca",
        "editorIndentGuide.activeBackground": "#FF0000",

        // "editorBracketPairGuide.background1": "#FF0000",
        // "editorBracketPairGuide.background2": "#FB78B5",
        // "editorBracketPairGuide.background3": "#4AACFB",
        // "editorBracketPairGuide.background4": "#B98BF5",
        // "editorBracketPairGuide.background5": "#DAD8D8",
        // "editorBracketPairGuide.background6": "#18EECE",
        "editorBracketHighlight.unexpectedBracket.foreground": "#FA8072"
    },
    "name": "Monokai 4nill"
}
